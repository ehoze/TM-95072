    <TableLayout android:id="@id/menu" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <TableRow>
            <TextView android:padding="3.0dip" android:layout_column="1" android:text="@string/table_layout_8_open" android:contentDescription="@string/table_layout_8_open" />
            <TextView android:gravity="right" android:padding="3.0dip" android:text="@string/table_layout_8_open_shortcut" android:contentDescription="@string/table_layout_8_open_shortcut" />
        </TableRow>
        <TableRow>
            <TextView android:padding="3.0dip" android:layout_column="1" android:text="@string/table_layout_8_save" android:contentDescription="@string/table_layout_8_save" />
            <TextView android:gravity="right" android:padding="3.0dip" android:text="@string/table_layout_8_save_shortcut" android:contentDescription="@string/table_layout_8_save_shortcut" />
        </TableRow>
        <TableRow>
            <TextView android:padding="3.0dip" android:layout_column="1" android:text="@string/table_layout_8_save_as" android:contentDescription="@string/table_layout_8_save_as" />
