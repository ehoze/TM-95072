        <TableRow>
            <TextView android:padding="3.0dip" android:text="@string/table_layout_8_x" android:contentDescription="@string/table_layout_8_x" />
            <TextView android:padding="3.0dip" android:text="@string/table_layout_8_export" android:contentDescription="@string/table_layout_8_export" />
            <TextView android:gravity="right" android:padding="3.0dip" android:text="@string/table_layout_8_export_shortcut" android:contentDescription="@string/table_layout_8_export_shortcut" />
        </TableRow>
    </TableLayout>
    <Button android:id="@id/toggle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/table_layout_8_toggle_stretch" android:contentDescription="@string/table_layout_8_toggle_stretch" />
